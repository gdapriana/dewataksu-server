model District {
    id          String  @id @default(cuid())
    name        String
    slug        String  @unique
    description String?
    coverId     String?
    cover       Image?  @relation(fields: [coverId], references: [id], onDelete: Cascade)

    destinations Destination[]

    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}
